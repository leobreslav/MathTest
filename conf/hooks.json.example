[
    {
      "id": "deploy_math_test",
      "execute-command": "/srv/math_test/app/tools/deploy.sh >> /srv/math_test/deploy_log.txt 2>> /srv/math_test/deploy_error_log.txt",
      "command-working-directory": "/srv/math_test/app",
      "response-message": "Executing deploy script...",
       "trigger-rule":
      {
        "and":
        [
          {
            "match":
            {
              "type": "payload-hash-sha1",
              "secret": "Place_your_secret_here",
              "parameter":
              {
                "source": "header",
                "name": "X-Hub-Signature"
              }
            }
          },
          {
            "match":
            {
              "type": "value",
              "value": "refs/heads/master",
              "parameter":
              {
                "source": "payload",
                "name": "ref"
              }
            }
          }
        ]
      }
    }
  ]